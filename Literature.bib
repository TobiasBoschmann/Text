@article{guandalini_comparative_2016,
	title = {Comparative assessment and safety issues in state-of-the-art hydrogen production technologies},
	volume = {41},
	issn = {0360-3199},
	url = {https://www.sciencedirect.com/science/article/pii/S0360319916302592},
	doi = {10/f89dfv},
	abstract = {This work discusses the development of a benchmark comparison among hydrogen production technologies, focussing on the assessment of their operating conditions by the point of view of the potential impact on safety. The analysis focuses on state-of-the-art, fully industrialized and commercial technologies, or at least with established early-market availability, not considering unconventional or emerging technologies still featuring a low technology readiness level. After a selection of hydrogen production technologies and their classification according to the possible feedstock, hydrogen purity and plant size, operating conditions and relative component sizes were calculated. Temperatures, pressures and chemical features are given in the most important points, as well as relative size of components, considering that these factors can be important by the point of view of safety. The qualitative comparison of the investigated technologies evidenced the presence of flammable and toxic mixtures at high temperature common to all the natural gas fed technologies, while gasifiers generally operates at lower temperature. ATR and gasifiers exhibit flammable and toxic gases at high pressure, as well as highly reactive streams (i.e. pure oxygen). This last is an aspect common also to electrolysis and POX. Common ranges for operating conditions are provided as a support for subsequent safety evaluation on how critical is a certain plant by the point of view of safety.},
	language = {en},
	number = {42},
	urldate = {2021-03-22},
	journal = {International Journal of Hydrogen Energy},
	author = {Guandalini, Giulio and Campanari, Stefano and Valenti, Gianluca},
	month = nov,
	year = {2016},
	keywords = {Benchmark, Electrolysis, Gasification, Hydrogen production, Operating conditions, Safety, Steam reforming},
	pages = {18901--18920},
	file = {ScienceDirect Snapshot:C\:\\Users\\tobias\\Zotero\\storage\\YY5YGEGW\\S0360319916302592.html:text/html},
}

@article{isenberg_energy_1981,
	title = {Energy conversion via solid oxide electrolyte electrochemical cells at high temperatures},
	volume = {3-4},
	issn = {0167-2738},
	url = {https://www.sciencedirect.com/science/article/pii/0167273881901272},
	doi = {10/cs8rrj},
	abstract = {The emergence of reliable and inexpensive deposition techniques for thin-film zirconia electrolytes, and for electronically conducting electrode connectors, has made possible the successful fabrication and testing of high-temperature solid oxide electrolyte (HTSOE) cells. Such cells can find application in electrical power generation (fuel cells) as well as in converting electrical energy into chemical energy (electrolysis). Neither individual cell component- nor interconnected cell stack performance-deterioration has been observed in life tests of over 5000 h at 1000°C. Temperature cycling of cell stacks from 1000°C to room temperature and back to 1000°C has demonstrated the mechanical soundness of the cell structure. This was achieved by proper matching of materials with respect to electrical, thermal, mechanical, and chemical characteristics. Electrochemical performance data have been obtained that form the basis on which to build future generator hardware. The long-range goal of the development program is the application of this fuel cell technology for cogeneration systems, i.e. ones that make use not only of the electrical energy but also of the high-quality heat, generated by a power plant.},
	language = {en},
	urldate = {2021-03-22},
	journal = {Solid State Ionics},
	author = {Isenberg, A. O.},
	month = aug,
	year = {1981},
	pages = {431--437},
	file = {ScienceDirect Snapshot:C\:\\Users\\tobias\\Zotero\\storage\\TE7U9RJW\\0167273881901272.html:text/html},
}

