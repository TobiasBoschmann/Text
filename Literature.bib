@book{redaktionsassistenz_2_co2-emissionsfaktoren_2016,
	title = {{CO2}-{Emissionsfaktoren} für fossile {Brennstoffe}},
	url = {https://www.umweltbundesamt.de/publikationen/co2-emissionsfaktoren-fuer-fossile-brennstoffe},
	abstract = {Deutschland ist verpflichtet, jährlich die nationalen Emissionen der Treibhausgase an die Europäische Union und an die Vereinten Nationen zu berichten. Über 80 \% der berichteten Treibhausgasemissionen in Deutschland entstehen aus der Verbrennung fossiler Brennstoffe, zum weit überwiegenden Teil in Form von Kohlendioxid. Um die Kohlendioxid-Emissionen zu berechnen benötigt man neben den Aktivitätsdaten auch passende Emissionsfaktoren, diese hängen wiederum von der Brennstoffqualität und der eingesetzten Menge ab. Aufgrund der Relevanz dieser Quellen werden für das deutsche Inventar keine internationalen Durchschnittswerte verwendet, sondern landesspezifische Emissionsfaktoren. Für deren Bestimmung ist eine umfangreiche Kenntnis der Brennstoffzusammensetzung, insbesondere der Kohlenstoffgehalte und Heizwerte unbedingt nötig.Die folgende Veröffentlichung gibt einen Überblick über die Qualität der wichtigsten in Deutschland eingesetzten Brennstoffe und die daraus berechneten CO2-Emissionsfaktoren. Da die Treibhausgasemissionen bis 1990 zurück berechnet werden müssen, werden auch Brennstoffe untersucht, die aktuell nicht mehr eingesetzt werden. Dazu werden Archivdaten verwendet, im Falle von Datenlücken werden Methoden angewendet, die eine Rückrechnung bis zum Basisjahr ermöglichen.},
	language = {de},
	urldate = {2021-05-10},
	publisher = {Umweltbundesamt},
	author = {Redaktionsassistenz 2, U. B. A.},
	month = sep,
	year = {2016},
	file = {Snapshot:C\:\\Users\\tobias\\Zotero\\storage\\YAN7JLYN\\co2-emissionsfaktoren-fuer-fossile-brennstoffe.html:text/html;Redaktionsassistenz 2 - 2016 - CO2-Emissionsfaktoren für fossile Brennstoffe.pdf:C\:\\Users\\tobias\\Zotero\\storage\\6H32J9RH\\Redaktionsassistenz 2 - 2016 - CO2-Emissionsfaktoren für fossile Brennstoffe.pdf:application/pdf},
